---
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <link href="{{ site.baseurl }}/tech/feed.xml" rel="self" type="application/atom+xml"/>
    <link href="{{ site.baseurl }}/tech/" rel="alternate" type="text/html"/>

    <updated> {{ site.time | date_to_xmlschema }} </updated>
    <id> {{ site.url }}{{ site.baseurl }}/tech/feed.xml </id>
    <title> {{ site.title }} tech category</title>
    <author>
        <name> {{ site.author }} </name>
    </author>

        {% for post in site.posts limit: 20 %}{% if post.category=="photography" %}
    <entry>
        <title>{{ post.title }}</title>
        <id>{{ site.url }}{{ site.baseurl }}{{ post.url }} </id>
        <link href="{{ site.baseurl }}{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <summary>
            {% if post.excerpt %}
            {{ post.excerpt | strip_html | lstrip | rstrip }}
            {% endif %}
        </summary>
    </entry>
        {% endif %}{% endfor %}
</feed>
